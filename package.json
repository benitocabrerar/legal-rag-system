{
  "name": "legal-rag-backend",
  "version": "1.0.0",
  "description": "Backend API for Legal RAG System",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
    "start": "tsx src/server.ts",
    "start:prod": "node --loader ts-node/esm src/server.ts",
    "postinstall": "node scripts/resolve-failed-migrations.cjs",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:resolve": "node scripts/migrate-with-resolve.js",
    "prisma:studio": "prisma studio",
    "seed:laws": "bun run scripts/seed-laws.ts",
    "add:admin": "node scripts/add-admin.cjs",
    "cleanup:duplicates": "tsx scripts/cleanup-duplicate-documents.ts",
    "generate:embeddings": "tsx src/scripts/generate-embeddings.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@aws-sdk/s3-request-presigner": "^3.929.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@langchain/anthropic": "^0.1.3",
    "@langchain/openai": "^0.0.19",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation-fastify": "^0.53.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@pinecone-database/pinecone": "^2.0.0",
    "@prisma/client": "^5.10.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "bullmq": "^5.63.0",
    "cron": "^4.3.4",
    "dd-trace": "^5.77.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.0",
    "fuse.js": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.25",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.16",
    "openai": "^4.28.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf.js-extract": "^0.2.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.13",
    "speakeasy": "^2.0.0",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@vitest/ui": "^4.0.8",
    "bun-types": "^1.0.0",
    "prisma": "^5.10.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  }
}
